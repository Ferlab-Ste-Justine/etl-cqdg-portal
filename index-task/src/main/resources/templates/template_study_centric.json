{
  "index_patterns" : ["study_centric*"],
  "priority" : 1,
  "template": {
    "settings" : {
      "number_of_shards" : 5,
      "analysis": {
        "normalizer": {
          "custom_normalizer": {
            "type": "custom",
            "char_filter": [],
            "filter": "lowercase"
          }
        }
      }
    },
    "mappings": {
      "properties": {
        "keyword": {
          "type": "keyword"
        },
        "description": {
          "type": "keyword"
        },
        "contact": {
          "properties": {
            "type": {
              "type": "keyword"
            },
            "value": {
              "type": "keyword"
            }
          }
        },
        "status": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword",
          "normalizer" : "custom_normalizer"
        },
        "domain": {
          "type": "keyword"
        },
        "population" : {
          "type" : "keyword"
        },
        "study_version" : {
          "type" : "keyword"
        },
        "data_types": {
          "type": "nested",
          "properties": {
            "data_type": {
              "type": "keyword"
            },
            "participant_count": {
              "type": "integer"
            }
          }
        },
        "data_categories": {
          "type": "nested",
          "properties": {
            "data_category": {
              "type": "keyword"
            },
            "participant_count": {
              "type": "integer"
            }
          }
        },
        "participant_count" : {
          "type" : "integer"
        },
        "file_count" : {
          "type" : "integer"
        },
        "data_category" : {
          "type" : "keyword"
        },
        "family_count" : {
          "type" : "integer"
        },
        "experimental_strategies" : {
          "type" : "keyword"
        },
        "hpo_terms" : {
          "type" : "keyword"
        },
        "mondo_terms" : {
          "type" : "keyword"
        },
        "icd_terms" : {
          "type" : "keyword"
        },
        "family_data" : {
          "type" : "boolean"
        },
        "data_access_codes": {
          "properties": {
            "access_requirements": {
              "type": "keyword"
            },
            "access_limitations": {
              "type": "keyword"
            }
          }
        },
        "internal_study_id" : {
          "type" : "keyword"
        },
        "study_code" : {
          "type" : "keyword",
          "normalizer" : "custom_normalizer"
        },
        "study_id" : {
          "type" : "keyword"
        },
        "release_id" : {
          "type" : "keyword"
        }
      }
    }
  }
}
