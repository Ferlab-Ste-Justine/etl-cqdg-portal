{
  "index_patterns": [
    "participant_centric*"
  ],
  "priority": 1,
  "template": {
    "settings": {
      "number_of_shards": 5,
      "analysis": {
        "normalizer": {
          "custom_normalizer": {
            "type": "custom",
            "char_filter": [],
            "filter": "lowercase"
          }
        }
      }
    },
    "mappings": {
      "dynamic_templates": [
        {
          "named_analyzers": {
            "match_mapping_type": "string",
            "path_match": "participant*_id",
            "mapping": {
              "type": "keyword",
              "normalizer": "custom_normalizer"
            }
          }
        },
        {
          "strings_as_keywords": {
            "match_mapping_type": "string",
            "mapping": {
              "type": "keyword"
            }
          }
        },
        {
          "object_as_nested_biospecimens": {
            "match_mapping_type": "object",
            "path_match": "*biospecimens*",
            "mapping": {
              "type": "nested"
            }
          }
        },
        {
          "object_as_nested_diagnoses": {
            "match_mapping_type": "object",
            "path_match": "diagnoses",
            "mapping": {
              "type": "nested"
            }
          }
        },
        {
          "object_as_nested_icd_tagged": {
            "match_mapping_type": "object",
            "path_match": "icd_tagged",
            "mapping": {
              "type": "nested"
            }
          }
        },
        {
          "object_as_nested_mondo": {
            "match_mapping_type": "object",
            "path_match": "mondo*",
            "mapping": {
              "type": "nested"
            }
          }
        },
        {
          "object_as_nested_observed_phenotype": {
            "match_mapping_type": "object",
            "path_match": "*observed_phenotype*",
            "mapping": {
              "type": "nested"
            }
          }
        },
        {
          "object_as_nested_family_relationships": {
            "match_mapping_type": "object",
            "path_match": "family_relationships",
            "mapping": {
              "type": "nested"
            }
          }
        },
        {
          "object_as_nested_files": {
            "match_mapping_type": "object",
            "path_match": "files",
            "mapping": {
              "type": "nested"
            }
          }
        }
      ]
    }
  }
}
