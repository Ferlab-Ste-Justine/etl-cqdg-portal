{
  "index_patterns": [
    "gene_suggestions*"
  ],
  "priority": 1,
  "template": {
    "settings": {
      "number_of_shards": "1",
      "index": {
        "max_result_window": 12000
      }
    },
    "mappings": {
      "properties": {
        "symbol": {
          "index": false,
          "type": "keyword"
        },
        "suggestion_id": {
          "index": false,
          "type": "keyword"
        },
        "ensembl_gene_id": {
          "index": false,
          "type": "keyword"
        },
        "suggest": {
          "max_input_length": 50,
          "analyzer": "standard",
          "preserve_position_increments": true,
          "type": "completion",
          "preserve_separators": true
        },
        "type": {
          "index": false,
          "type": "keyword"
        }
      }
    }
  }
}